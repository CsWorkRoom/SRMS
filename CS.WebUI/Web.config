<?xml version="1.0" encoding="utf-8"?>
<!--
  For more information on how to configure your ASP.NET application, please visit
  http://go.microsoft.com/fwlink/?LinkId=301880
  -->
<configuration>
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="UpdateAppPermissionServicesSoapBinding" />
        <binding name="CheckAiuapTokenSoapSoapBinding" />
      </basicHttpBinding>
    </bindings>
    <client>
      <!--4A组织结构变更-->
      <endpoint address="http://10.109.209.29:18091/uap/services/UpdateAppPermissionServices"
          binding="basicHttpBinding" bindingConfiguration="UpdateAppPermissionServicesSoapBinding"
          contract="DepartmentServiceReference.UpdateAppPermissionServices"
          name="UpdateAppPermissionServices" />

      <!--4A单点登录：新计件/老计件共用一个接口-->
      <endpoint address="http://10.109.209.100:9081/uac/services/CheckAiuapTokenSoap"
          binding="basicHttpBinding" bindingConfiguration="CheckAiuapTokenSoapSoapBinding"
          contract="Login4AServiceReference.CommonTokenService" name="CheckAiuapTokenSoap" />
      
      <!--需要做计件6服务器到10.109.209.29:18091端口的网络策略-->
    </client>
  </system.serviceModel>
  <appSettings>

    <add key="webpages:Version" value="3.0.0.0"/>
    <add key="webpages:Enabled" value="false"/>
    <add key="ClientValidationEnabled" value="true"/>
    <add key="UnobtrusiveJavaScriptEnabled" value="true"/>

    <!--日志等级，填写数字：(高) OFF【0】 > FATAL【1】 > ERROR【2】 > WARN【3】 > INFO【4】 > DEBUG【5】 (低) -->
    <add key="LogWriteLevel" value="5"/>
    <!--数据库类型-->
    <add key="DataBaseType" value="Oracle"/>
    <!--<add key="DataBaseType" value="DB2" />-->
    <!--数据库连接字符串-->
    <add key="ConnString" value="User ID=SRMS;Password=SRMS;Data Source=(DESCRIPTION=(ADDRESS=(PROTOCOL=TCP)(HOST=127.0.0.1)(PORT=1521))(CONNECT_DATA=(SERVICE_NAME=ORCL)));Persist Security Info=True;"/>
    <!--<add key="ConnString" value="Server=114.116.50.232:50000;UID=C##DY_FAS2;PWD=easyman123@@@;DataBase=DMDY;"/>-->
    <!--<add key="ConnString" value="Server=114.116.50.232:50000;UID=C##CSO;PWD=CS@@@;DataBase=DMDY;"/>-->
    <!--系统表主键是否自增-->
    <add key="KeyFieldIsAutoIncrement" value="false"/>
    
    <!--附件存放路径（框架中使用）-->
    <add key="AttachmentPath" value="D:\www\QSS\Web\Attachment"/>

    <!--标签管理附件的路径-->
    <add key="LabelPath" value="D:\www\QSS\Web\LabelPath"/>
    <!--指标管理附件的路径-->
    <add key="TargetPath" value="D:\www\QSS\Web\TargetPath"/>
    <!--指标过滤管理附件的路径-->
    <add key="FilterPath" value="D:\www\QSS\Web\FilterPath"/>
    <!--计件政策管理附件的路径-->
    <add key="JjPolicyPath" value="D:\www\QSS\Web\JjPolicyPath"/>
    <!--抢盘政策管理附件的路径-->
    <add key="QpPolicyPath" value="D:\www\QSS\Web\QpPolicyPath"/>
    <!--抢盘模板管理附件的路径-->
    <add key="QpTemplatePath" value="D:\www\QSS\Web\QpTemplatePath"/>
    <!--获取分享政策管理附件的路径-->
    <add key="FxPolicyPath" value="D:\www\QSS\Web\FxPolicyPath"/>
    
    <!--是否与4A对接：true=对接；false=不对接 -->
    <add key="IsConnectTo4A" value="false"/>
    <!--审计接口地址：目前21010端口已坏，请使用19094或21030端口做策略-->
    <!--<add key="OpLogAddress" value="http://10.109.209.90:21010/gather/services/AioxPort" />-->
    <add key="OpLogAddress" value="http://10.109.209.90:19094/gather/services/AioxPort" />
    <!--<add key="OpLogAddress" value="http://10.109.209.90:21030/gather/services/AioxPort" />-->
  </appSettings>
  <system.web>
    <compilation debug="true" targetFramework="4.5.2"/>
    <httpRuntime targetFramework="4.5.2" maxRequestLength="40960000" executionTimeout="600"/>
    <httpModules>
      <add name="ApplicationInsightsWebTracking" type="Microsoft.ApplicationInsights.Web.ApplicationInsightsHttpModule, Microsoft.AI.Web"/>
    </httpModules>
  </system.web>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="System.Web.Helpers" publicKeyToken="31bf3856ad364e35"/>
        <bindingRedirect oldVersion="1.0.0.0-3.0.0.0" newVersion="3.0.0.0"/>
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="System.Web.WebPages" publicKeyToken="31bf3856ad364e35"/>
        <bindingRedirect oldVersion="1.0.0.0-3.0.0.0" newVersion="3.0.0.0"/>
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="System.Web.Mvc" publicKeyToken="31bf3856ad364e35"/>
        <bindingRedirect oldVersion="1.0.0.0-5.2.3.0" newVersion="5.2.3.0"/>
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
  <system.codedom>
    <compilers>
      <compiler language="c#;cs;csharp" extension=".cs"
        type="Microsoft.CodeDom.Providers.DotNetCompilerPlatform.CSharpCodeProvider, Microsoft.CodeDom.Providers.DotNetCompilerPlatform, Version=1.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35"
        warningLevel="4" compilerOptions="/langversion:6 /nowarn:1659;1699;1701"/>
      <compiler language="vb;vbs;visualbasic;vbscript" extension=".vb"
        type="Microsoft.CodeDom.Providers.DotNetCompilerPlatform.VBCodeProvider, Microsoft.CodeDom.Providers.DotNetCompilerPlatform, Version=1.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35"
        warningLevel="4" compilerOptions="/langversion:14 /nowarn:41008 /define:_MYTYPE=\&quot;Web\&quot; /optionInfer+"/>
    </compilers>
  </system.codedom>
  <system.webServer>
    <validation validateIntegratedModeConfiguration="false"/>
    <modules>
      <remove name="ApplicationInsightsWebTracking"/>
      <add name="ApplicationInsightsWebTracking" type="Microsoft.ApplicationInsights.Web.ApplicationInsightsHttpModule, Microsoft.AI.Web"
        preCondition="managedHandler"/>
    </modules>

    <httpErrors errorMode="Custom" existingResponse="Replace">
      <!--
      <remove statusCode="404"/>
      <error statusCode="404"  path="/AfError/Index?code=404" responseMode="Redirect"/>
      -->
    </httpErrors>

  </system.webServer>
</configuration>
